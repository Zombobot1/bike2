{
  "name": "front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "cross-env DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "react-scripts --max_old_space_size=2048 build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{tsx,ts} --fix --max-warnings=0",
    "compile": "rimraf build && tsc",
    "check": "npm run lint && npm run compile",
    "before-commit": "npm run check && git add .",
    "after-commit": "npm run check",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@nivo/core": "^0.67.0",
    "@nivo/line": "^0.67.0",
    "@nivo/pie": "^0.67.0",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.1",
    "class-validator": "^0.12.2",
    "classnames": "^2.2.6",
    "jotai": "^0.16.6",
    "lodash": "^4.17.20",
    "popper.js": "^1.16.1",
    "query-string": "^6.13.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.12.1",
    "react-router-dom": "^5.2.0",
    "screenfull": "^5.1.0",
    "slugify": "^1.5.3",
    "swiper": "^6.5.9"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@popperjs/core": "^2.6.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/classnames": "^2.2.11",
    "@types/jest": "^26.0.15",
    "@types/lodash": "^4.14.167",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-unused-imports": "^1.1.1",
    "husky": "^6.0.0",
    "is-ci": "^2.0.0",
    "msw": "^0.27.1",
    "prettier": "^2.2.1",
    "react-scripts": "^4.0.3",
    "rimraf": "^2.7.1",
    "sass": "^1.34.0",
    "sass-loader": "^10.1.1",
    "style-loader": "^1.3.0",
    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
