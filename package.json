{
  "name": "uni",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "start": "cross-env DISABLE_ESLINT_PLUGIN=true react-scripts start",
    "build": "react-scripts --max_old_space_size=2048 build",
    "serve": "react-scripts build && serve -s build",
    "test": "react-scripts test --runInBand",
    "test-ci": "react-scripts test --watchAll=false --silent",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{tsx,ts} --fix --max-warnings=0",
    "check": "npm run lint && tsc && npm run test-ci",
    "commit-dangerously": "git add . && git commit --no-verify -m",
    "pre-commit": "npm run check && git add .",
    "pre-ci": "npm run check",
    "prepare": "is-ci || husky install"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^5.0.0-beta.0",
    "@material-ui/icons": "^5.0.0-beta.0",
    "@material-ui/styled-engine-sc": "^5.0.0-beta.0",
    "firebase": "^8.6.8",
    "jotai": "^1.2.2",
    "lodash": "^4.17.20",
    "query-string": "^6.13.7",
    "react": "^17.0.2",
    "react-contenteditable": "^3.3.5",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.4",
    "react-error-boundary": "^3.1.3",
    "react-flatten-children": "^1.1.2",
    "react-masonry-css": "^1.0.16",
    "react-query": "^3.12.1",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.2",
    "slugify": "^1.5.3",
    "uuid": "^8.3.2",
    "workbox-background-sync": "^5.1.4",
    "workbox-broadcast-update": "^5.1.4",
    "workbox-cacheable-response": "^5.1.4",
    "workbox-core": "^5.1.4",
    "workbox-expiration": "^5.1.4",
    "workbox-google-analytics": "^5.1.4",
    "workbox-navigation-preload": "^5.1.4",
    "workbox-precaching": "^5.1.4",
    "workbox-range-requests": "^5.1.4",
    "workbox-routing": "^5.1.4",
    "workbox-strategies": "^5.1.4",
    "workbox-streams": "^5.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.167",
    "@types/node": "14.14.33",
    "@types/react": "17.0.3",
    "@types/react-dom": "17.0.3",
    "@types/react-router-dom": "^5.1.6",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "~4.28.3",
    "@typescript-eslint/parser": "~4.28.3",
    "cross-env": "^7.0.3",
    "dotenv": "8.2.0",
    "eslint": "7.22.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "eslint-plugin-unused-imports": "^1.1.1",
    "husky": "^7.0.1",
    "is-ci": "^2.0.0",
    "msw": "^0.27.2",
    "prettier": "^2.3.1",
    "react-scripts": "^4.0.3",
    "rimraf": "^2.7.1",
    "typescript": "~4.3.5"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
