{
  "version": 2,
  "projects": {
    "front": {
      "root": "apps/front",
      "sourceRoot": "apps/front/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": ["{options.outputPath}"],
          "options": {
            "command": "node ../../node_modules/react-app-rewired/bin/index.js build",
            "cwd": "apps/front",
            "outputPath": "dist/apps/front"
          }
        },
        "serve": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/react-app-rewired/bin/index.js start",
            "cwd": "apps/front"
          }
        },
        "lint": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "eslint src/**/*.tsx src/**/*.ts --fix --max-warnings=0",
            "cwd": "apps/front"
          }
        },
        "compile": {
          "executor": "./tools/executors/tsc:tsc"
        },
        "test": {
          "executor": "@nrwl/workspace:run-commands",
          "outputs": [],
          "options": {
            "command": "node ../../node_modules/react-app-rewired/bin/index.js test --watchAll=false",
            "cwd": "apps/front"
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "front"
}
